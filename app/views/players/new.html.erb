<%= form_for :player, url: players_path do |f| %>
	<p>
		<%= f.label :username %><br>
		<%= f.text_field :username %><br>
	</p>

	<p>
		<%= f.label :email %><br>
		<%= f.email :email %><br>
	</p>

	<p>
		<%= f.label :password %><br>
		<%= f.password_field :password %>
	</p>

	<p>
		<%= f.label :password_confirmation %><br>
		<%= f.password_field :password_confirmation %><br>
	</p>

	<p>
		<%= f.submit %>
	</p>

<% end %>